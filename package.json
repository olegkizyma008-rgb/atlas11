{
    "name": "atlas5-kontur",
    "version": "2.0.0",
    "main": "out/main/index.js",
    "scripts": {
        "dev": "npm run copy-assets && electron-vite dev",
        "build": "electron-vite build && npm run copy-assets",
        "preview": "electron-vite preview",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "kontur:dev": "AG=true tsc -w",
        "kontur:start": "AG=true node out/main/index.js",
        "kontur:levitate": "node out/kontur/ag-levitate.js",
        "copy-assets": "mkdir -p out/kontur/organs out/kontur/ag && cp src/kontur/organs/*.py out/kontur/organs/ && cp src/kontur/ag/*.py out/kontur/ag/",
        "deploy": "gcloud run deploy atlas11-kontur --source . --allow-unauthenticated"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.5.0",
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@google/genai": "^1.31.0",
        "@google/generative-ai": "^0.24.1",
        "@modelcontextprotocol/sdk": "^1.24.3",
        "@modelcontextprotocol/server-filesystem": "^2025.11.25",
        "@tanstack/react-query": "^4.18.0",
        "@trpc/client": "^10.0.0",
        "@trpc/react-query": "^10.0.0",
        "@trpc/server": "^10.0.0",
        "better-sqlite3": "^9.6.0",
        "bottleneck": "^2.19.5",
        "clsx": "^2.0.0",
        "date-fns": "^2.30.0",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.29.5",
        "electron-trpc": "^0.5.0",
        "execa": "^8.0.0",
        "express": "^4.18.0",
        "lucide-react": "^0.290.0",
        "mime": "^4.1.0",
        "openai": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.0",
        "tailwind-merge": "^2.0.0",
        "uuid": "^9.0.0",
        "ws": "^8.18.3",
        "yaml": "^2.3.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/mime": "^3.0.4",
        "@types/node": "^18.0.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.0.0",
        "autoprefixer": "^10.4.0",
        "drizzle-kit": "^0.20.18",
        "electron": "^28.0.0",
        "electron-vite": "^1.0.28",
        "postcss": "^8.4.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.0",
        "vite": "^4.0.0",
        "vitest": "^1.0.0"
    }
}

BRAIN_PROVIDER=copilot
# Replace the values below with your own API keys in a local `.env` file
BRAIN_API_KEY=REPLACE_WITH_BRAIN_API_KEY
BRAIN_MODEL=gpt-4o

# Global Copilot key for all services
COPILOT_API_KEY=REPLACE_WITH_COPILOT_API_KEY

TTS_API_KEY=REPLACE_WITH_TTS_API_KEY

TTS_PROVIDER=gemini

TTS_MODEL=gemini-2.5-flash-preview-tts

STT_PROVIDER=gemini

STT_MODEL=gemini-2.5-flash

STT_API_KEY=REPLACE_WITH_STT_API_KEY

VISION_MODE=live

VISION_PROVIDER=gemini

VISION_MODEL=gemini-2.5-flash-native-audio-preview-09-2025

VISION_API_KEY=REPLACE_WITH_VISION_API_KEY

VISION_FALLBACK_PROVIDER=copilot

NODE_ENV=development

VISION_LIVE_PROVIDER=gemini

VISION_LIVE_API_KEY=REPLACE_WITH_VISION_LIVE_API_KEY

VISION_LIVE_MODEL=gemini-2.5-flash-native-audio-preview-09-2025

VISION_ONDEMAND_PROVIDER=copilot

VISION_ONDEMAND_API_KEY=REPLACE_WITH_VISION_ONDEMAND_API_KEY

VISION_ONDEMAND_MODEL=gpt-4o

REASONING_API_KEY=REPLACE_WITH_REASONING_API_KEY

REASONING_PROVIDER=copilot

REASONING_MODEL=gpt-4.1

TTS_FALLBACK_PROVIDER=ukrainian

STT_FALLBACK_PROVIDER=web

UKRAINIAN_TTS_VOICE=Tetiana

UKRAINIAN_VOICE_GRISHA=Mykyta

UKRAINIAN_VOICE_ATLAS=oleksa

UKRAINIAN_VOICE_TETYANA=Tetiana

TTS_FALLBACK2_PROVIDER=web

APP_THEME=dark

LOG_LEVEL=info

DEBUG=false

# ============ EXECUTION ENGINE ============
EXECUTION_ENGINE=python-bridge
GEMINI_API_KEY=REPLACE_WITH_GEMINI_API_KEY
BRAIN_FALLBACK_PROVIDER=gemini

# TEST MODE - keep `GRISHA_TEST_MODE` false in production
GRISHA_TEST_MODE=false
# KONTUR V3 - MULTI-PROVIDER CONFIGURATION
# Кожен сервіс може використовувати різні провайдери з fallback
# Провайдери: gemini | copilot | mistral | openai | anthropic

# ============ GLOBAL FALLBACK KEYS ============
# Ці ключі використовуються якщо сервіс-специфічні не встановлені
GEMINI_API_KEY=your_gemini_key_here
GOOGLE_API_KEY=your_google_key_here

# ============ BRAIN (чат/планування) ============
# BRAIN_PROVIDER=gemini
# BRAIN_FALLBACK_PROVIDER=copilot
# BRAIN_MODEL=gemini-2.5-flash
# BRAIN_API_KEY=xxx

# ============ TTS (голос) ============
# TTS_PROVIDER=gemini
# TTS_FALLBACK_PROVIDER=elevenlabs
# TTS_MODEL=gemini-2.5-flash-preview-tts
# TTS_API_KEY=xxx

# ============ STT (транскрипція) ============
# STT_PROVIDER=gemini
# STT_FALLBACK_PROVIDER=whisper
# STT_MODEL=gemini-2.5-flash
# STT_API_KEY=xxx

# ============ VISION (Grisha Visual Analysis) ============
# MODE: який режим активний - 'live' або 'on-demand'
VISION_MODE=live
# FALLBACK_MODE: резервний режим якщо основний недоступний (live, on-demand, або пусто для відсутнього)
# VISION_FALLBACK_MODE=on-demand

# --- Live Stream (Gemini Live - реальний час) ---
VISION_LIVE_PROVIDER=gemini
# VISION_LIVE_FALLBACK_PROVIDER=
VISION_LIVE_MODEL=gemini-2.5-flash-native-audio-preview-09-2025
# VISION_LIVE_API_KEY=xxx  # Uses GEMINI_LIVE_API_KEY by default
GEMINI_LIVE_API_KEY=your_live_key_here

# --- On-Demand (скріншот після завдання - Copilot/GPT-4o) ---
VISION_ONDEMAND_PROVIDER=copilot
VISION_ONDEMAND_FALLBACK_PROVIDER=gemini
VISION_ONDEMAND_MODEL=gpt-4o
# VISION_ONDEMAND_API_KEY=xxx  # Uses COPILOT_API_KEY by default
# Для видалення fallback залиш пусте: VISION_ONDEMAND_FALLBACK_PROVIDER=

# ============ REASONING (глибоке мислення - Gemini 3) ============
# REASONING_PROVIDER=gemini
# REASONING_MODEL=gemini-3-pro-preview
REASONING_API_KEY=your_reasoning_key_here

# ============ ДОДАТКОВІ ПРОВАЙДЕРИ ============
# MISTRAL_API_KEY=xxx
# OPENAI_API_KEY=xxx
# ANTHROPIC_API_KEY=xxx
# ELEVENLABS_API_KEY=xxx

# ============ APP SETTINGS ============
NODE_ENV=development

# ============ EXECUTION ENGINE ============
# engine: 'python-bridge' (Open Interpreter) | 'native' (MCP/Standard)
EXECUTION_ENGINE=python-bridge
